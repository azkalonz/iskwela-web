[32m+[m[32mvar mysql = require("mysql");[m
[32m+[m
[32m+[m[32mvar pool = mysql.createPool({[m
[32m+[m[32m  connectionLimit: process.env.mysql_connection_pool_Limit, // default:10[m
[32m+[m[32m  host: process.env.mysql_host,[m
[32m+[m[32m  user: process.env.mysql_user,[m
[32m+[m[32m  password: process.env.mysql_password,[m
[32m+[m[32m  database: process.env.mysql_database,[m
[32m+[m[32m  port: process.env.mysql_port,[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mmodule.exports = {[m
[32m+[m[32m  all: () => {[m
[32m+[m[32m    return new Promise((resolve, reject) => {[m
[32m+[m[32m      pool.query("SELECT * FROM users", (err, res) => {[m
[32m+[m[32m        if (err) return reject(err);[m
[32m+[m[32m        resolve(res);[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m[32m  isUserRegistered: (email) => {[m
[32m+[m[32m    return new Promise((resolve, reject) => {[m
[32m+[m[32m      pool.query("SELECT * FROM users WHERE ?", { email: email }, (err, res) =>[m
[32m+[m[32m        err ? reject(err) : resolve(res.length && true)[m
[32m+[m[32m      );[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m[32m  registerUser: (user) => {[m
[32m+[m[32m    return new Promise((resolve, reject) => {[m
[32m+[m[32m      let data = {[m
[32m+[m[32m        number: Math.floor(Math.random() * 9909),[m
[32m+[m[32m        name: user.nickname,[m
[32m+[m[32m        role: "Student",[m
[32m+[m[32m        email: user.email,[m
[32m+[m[32m      };[m
[32m+[m[32m      pool.query("INSERT INTO users SET ?", data, (err, res) =>[m
[32m+[m[32m        err ? reject(err) : resolve(true)[m
[32m+[m[32m      );[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m[32m  joinClass: (email, classCode) => {[m
[32m+[m[32m    return new Promise((resolve, reject) => {[m
[32m+[m[32m      let prevClasses = "";[m
[32m+[m[32m      pool.query([m
[32m+[m[32m        "SELECT classes FROM users WHERE email = ?",[m
[32m+[m[32m        email,[m
[32m+[m[32m        (err, res) => {[m
[32m+[m[32m          if (res[0].classes.split(",").indexOf(classCode) >= 0) {[m
[32m+[m[32m            resolve(res);[m
[32m+[m[32m            return;[m
[32m+[m[32m          }[m
[32m+[m[32m          if (err) reject(err);[m
[32m+[m[32m          prevClasses = res[0].classes;[m
[32m+[m[32m          if (prevClasses.length) {[m
[32m+[m[32m            prevClasses = [prevClasses + "," + classCode];[m
[32m+[m[32m          } else {[m
[32m+[m[32m            prevClasses = [classCode];[m
[32m+[m[32m          }[m
[32m+[m[32m          pool.query([m
[32m+[m[32m            "UPDATE users SET classes = ? WHERE email = ?",[m
[32m+[m[32m            [prevClasses.toString(), email],[m
[32m+[m[32m            (e, r) => {[m
[32m+[m[32m              resolve(r);[m
[32m+[m[32m            }[m
[32m+[m[32m          );[m
[32m+[m[32m        }[m
[32m+[m[32m      );[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m[32m  getUserInfo: (email) => {[m
[32m+[m[32m    return new Promise((resolve, reject) => {[m
[32m+[m[32m      let buffer = null;[m
[32m+[m[32m      pool.query("SELECT * FROM users WHERE email = ?", email, (err, res) => {[m
[32m+[m[32m        if (err) reject(err);[m
[32m+[m[32m        else {[m
[32m+[m[32m          buffer = JSON.parse(JSON.stringify(res[0]));[m
[32m+[m[32m          if (!buffer.classes.length) {[m
[32m+[m[32m            buffer.classes = [];[m
[32m+[m[32m            resolve(buffer);[m
[32m+[m[32m            return;[m
[32m+[m[32m          }[m
[32m+[m[32m          let classNumbers =[m
[32m+[m[32m            buffer.classes.indexOf(",") >= 0[m
[32m+[m[32m              ? buffer.classes.split(",")[m
[32m+[m[32m              : [buffer.classes];[m
[32m+[m[32m          buffer.classes = [];[m
[32m+[m[32m          classNumbers.map((c, i) => {[m
[32m+[m[32m            pool.query("SELECT * FROM classes WHERE number = ?", c, (e, r) => {[m
[32m+[m[32m              if (e) reject(err);[m
[32m+[m[32m              else if (r[0]) {[m
[32m+[m[32m                if (!r[0].img.length)[m
[32m+[m[32m                  r[0].img =[m
[32m+[m[32m                    "https://cdn.shopify.com/s/files/1/0010/9215/7503/t/5/assets/MothersDay-enchroma.jpg";[m
[32m+[m[32m                buffer.classes.push(r);[m
[32m+[m[32m              }[m
[32m+[m[32m              if (i == classNumbers.length - 1) {[m
[32m+[m[32m                resolve(buffer);[m
[32m+[m[32m              }[m
[32m+[m[32m            });[m
[32m+[m[32m          });[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m[32m};[m
